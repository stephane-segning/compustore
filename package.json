{
  "name": "computer-nextjs",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "vercel-build": "yarn db:migrate && yarn build",
    "build": "next build",
    "check": "next lint && tsc --noEmit",
